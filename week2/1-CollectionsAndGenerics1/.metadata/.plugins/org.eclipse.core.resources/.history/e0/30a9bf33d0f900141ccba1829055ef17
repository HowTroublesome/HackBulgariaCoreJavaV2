
public class Main {
    public static void main(String[] arguments) {
        StatisticsImpl someStatistics = new StatisticImpl();
        
        someStatistics.add(13);
        someStatistics.add(18);
        someStatistics.add(13);
        someStatistics.add(14);
        someStatistics.add(13);
        someStatistics.add(16);
        someStatistics.add(14);
        someStatistics.add(21);
        someStatistics.add(13);
        
        System.out.println(someStatistics);
        System.out.println();
        
        System.out.println("Mean: " + someStatistics.getMean());
        System.out.println("Median: " + someStatistics.getMedian());
        System.out.println("Mode: " + someStatistics.getMode());
        System.out.println("Range: " + someStatistics.getRange());
    }
}
